personal {!id, inserted timestamp} {
  inventory {inventory_specific_id} {
    stock {price numeric(5)}
      -> physical_item

    physical_item {location point, dimension}
      1->1 item

    item {name, description?, type {'generic', 'food', 'tool'}}
  }

  skills {
    skill {name}

    table {attribute} -> skill
  }

  some_other_schema {
    with {a, whole, bunch}

    of {stuff}
      -> but_not

    but_not {that, much}
      -> inventory.stock
  }
}